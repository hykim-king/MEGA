<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pcwk.ehr.findid.FindIdDao">
    <select id="findUserId" parameterType="com.pcwk.ehr.findid.FindIdDTO" resultType="String">
        SELECT USER_ID
        FROM MEMBERSHIP
        WHERE EMAIL = #{email}
          AND TO_CHAR(BIRTH, 'YYYY-MM-DD') = #{birth}
    </select>
</mapper>